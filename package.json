{
  "name": "delidev-workspace",
  "private": true,
  "packageManager": "pnpm@10.26.2",
  "scripts": {
    "dev:local": "PUBLIC_DEFAULT_MODE=local PUBLIC_SKIP_MODE_SELECTION=true pnpm dev:inner",
    "dev:remote": "PUBLIC_SKIP_MODE_SELECTION=true pnpm dev:inner",
    "dev:inner": "turbo run dev",
    "dev:db": "podman compose up -d",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "prepare": "lefthook install; ./scripts/prepare-apps.sh"
  },
  "devDependencies": {
    "lefthook": "^2.0.14",
    "turbo": "^2.7.4"
  }
}
